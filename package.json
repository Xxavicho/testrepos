{
  "name": "@kushki/usrv-card",
  "version": "1.0.0",
  "description": "usrv-card",
  "main": "handler.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint:duplicate && npm run lint:check && npm run test:unit"
    }
  },
  "scripts": {
    "deploy": "serverless deploy -v",
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check",
    "lint:check": "tslint -t stylish --project tsconfig.json -c tslint.json 'src/**/*.ts' && npm run tsc:check",
    "lint:duplicate": "npx jscpd ./src",
    "lint:fix": "tslint -t stylish --fix --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "lint:format": "prettier --write \"./**/*.{ts,js,json,yml,yaml}\" \"./.{pull_review.kushkirc,lintstagedrc,nycrc,prettierrc}\"",
    "package": "serverless package",
    "sonar": "nyc report --reporter=lcov",
    "test": "npm run test:unit && npm run test:coverage",
    "test:coverage": "nyc mocha --opts ./mocha.nyc.opts src/**/*.spec.ts",
    "test:integration": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "mocha --opts ./mocha.opts src/**/*.spec.ts",
    "test:watch": "mocha --watch --opts ./mocha.opts src/**/*.spec.ts",
    "tsc:check": "tsc --project tsconfig.check.json --noEmit",
    "tsc:interface": "kushki json2ts",
    "validate": "npm run depcheck && npm list && npm run lint && npm run test",
    "depcheck": "depcheck . --ignores=\"serverless-*,@types/*,@kushki/*,prettier,@sendgrid/helpers,aws-lambda,aws-xray-sdk-core,acorn,tsutils,rollbar,chai,husky,lint-staged,ajv,ts-loader,tslint-config-prettier,infrastructure,constant,middleware,core,gateway,service,error,moment-timezone,dot-object,moment,lodash,faker,binlookup,camelcase-keys,change-case,nano-seconds,string-similarity,uuid,repository,types\""
  },
