{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://ksh/TokensCardResponse",
  "title": "TokensCardResponse",
  "type": "object",
  "properties": {
    "token": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{32}$"
    },
    "settlement": {
      "type": "number"
    }
  },
  "required": ["token"]
}
